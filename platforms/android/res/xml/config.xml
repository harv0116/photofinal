<?xml version='1.0' encoding='utf-8'?>
<widget id="ca.edumedia.m.harv0116" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <icon density="mdpi" src="www/icons/plogo_mdpi_48x48.png" />
    <icon density="hdpi" src="www/icons/plogo_hdpi_72x72.png" />
    <icon density="xhdpi" src="www/icons/plogo_xhdpi_96x96.png" />
    <icon density="xxhdpi" src="www/icons/plogo_xxhdpi.png" />
    <name>Paul and Anna Final Project</name>
    <description>
        An Apache Cordova application that takes and displays photos.
    </description>
    <author email="harv0116@algonquinlive.com">
        Paul Harvey and Anna Ioudovskaya
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="Fullscreen" value="true" />
    <preference name="orientation" value="landscape" />
</widget>
